                  <!-- /.image -->
                  <div class="title"><a href="#" >{{ x._id.kpi_name }}</a></div>
                  <div class="entry-content">
                  		<ul class="tags">
		              			<li><a href="#" class="tag">Ecobank</a><span class="label label-primary">24%</span></li>
		              			<li><a href="#" class="tag">Fidelity Bank</a><span class="label label-primary">30%</span></li>
		        				<li><a href="#" class="tag">Standard Chartered</a><span class="label label-primary">33%</span></li>
		        		</ul>
        		  </div>
			    </div>
			</div>

            <div class="col-md-3" ng-repeat="">
                <div class="deal-entry blue">
